class Guy{
    string name;
    string money;
    string car;
    string home;
    
    
    public Guy(string name, string money, string car, string home){
        this.name = name;
        this.money = money;
        this.car = car;
        this.home = home;
    }
     public string toString()
     {
         return "there name is " + this.name;
         return "there car is a " + this.car;
         return "there home is a " + this.home;
         return "the amount of money they have is " + this.money;
     }
}
