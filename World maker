using System;

public class HelloWorld
{
    public static void Main(string[] args)
    {
        Console.WriteLine ("make a world, type yes, to start.");
        string start = Console.ReadLine();
        if (start == "yes"){
                Console.WriteLine ("got it");
                Random rand = new Random();
                string[] liquid = {"urine", "oil", "sea water", "swamp water", "lava", "acid", "clear water", "yummy water", "emergency"};
                int liquidpick = rand.Next(0,9);
                int treespick = rand.Next(0,9);
                int groundpick = rand.Next(0,9);
                int peoplepick = rand.Next(0,9);
                int animalpick = rand.Next(0,9);
                Console.WriteLine(liquid[liquidpick]);
                Console.WriteLine(liquidpick);
                
                Console.WriteLine(liquid[treepick]);
                Console.WriteLine(treespick);
                Console.WriteLine(liquid[groundpick]);
                Console.WriteLine(groundpick);
                Console.WriteLine(liquid[peoplepick]);
                Console.WriteLine(peoplepick);
                Console.WriteLine(liquid[animalpick]);
                Console.WriteLine(animalpick);
        
            }
        }
    }
    
    
    
    
    
    
